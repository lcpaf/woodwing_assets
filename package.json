{
    "name": "woodwing-assets",
    "version": "1.1.1",
    "description": "Tools to connect to a WoodWing Assets Server API",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "test": "jest --config jestconfig.json",
        "build": "tsc",
        "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
        "lint": "tslint -p tsconfig.json",
        "prepare": "npm run build",
        "prepublishOnly": "npm run lint",
        "preversion": "npm run lint",
        "version": "npm run format && git add -A src",
        "postversion": "git push && git push --tags",
        "webhookTest": "$(npm bin)/ts-node src/__tests__/startServer.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/lcpaf/woodwing_assets.git"
    },
    "keywords": [
        "ProPublish",
        "WoodWing",
        "Elvis",
        "DAM",
        "Assets"
    ],
    "author": "Luis Ferreira",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/lcpaf/woodwing_assets/issues"
    },
    "homepage": "https://github.com/lcpaf/woodwing_assets#readme",
    "devDependencies": {
        "@types/jest": "^29.5.11",
        "@types/node": "^20.10.4",
        "jest": "^29.7.0",
        "prettier": "^2.8.8",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.2",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^5.3.3"
    },
    "files": [
        "lib/**/*"
    ],
    "dependencies": {
        "@types/bluebird": "^3.5.42",
        "@types/express": "^4.17.21",
        "@types/logger": "0.0.5",
        "@types/request": "^2.48.12",
        "@types/safe-compare": "^1.1.2",
        "@types/tmp": "^0.2.6",
        "bluebird": "^3.7.2",
        "express": "^4.18.2",
        "fs": "0.0.2",
        "http": "^0.0.1-security",
        "https": "^1.0.0",
        "request": "^2.88.2",
        "safe-compare": "^1.1.4",
        "stream": "0.0.2",
        "tmp": "^0.2.1"
    }
}
